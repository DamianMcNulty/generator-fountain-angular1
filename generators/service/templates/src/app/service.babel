class <%- serviceName %> {
<% if (modules === 'webpack') { -%>
<% include modules/service.ngInject.babel %>
<% } -%>
  constructor($http) {
    this.$http = $http;
  }

  getData() {
    return this.$http.get('api/data/');
  }
}

<% if (modules === 'inject') { -%>
<% include inject/service.babel %>
<% } else { -%>
<% include modules/service.babel %>
<% } -%>
